generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model anime {
  id              String @id @default(auto()) @map("_id") @db.ObjectId
  chaptersRead    String
  hiatus          String
  img             String
  name            String
  nextReleaseDate String
  pageNumber      String
  releaseSchedule String
  startDate       String
  status          String
  totalChapters   String
}
